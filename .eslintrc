{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "document": true,
    "window": true,
    "Event": true,
    "FileReader": true,
    "_": false,
    "FormData": true,
    "Blob": true
  },
  "rules": {
    "max-len": [
      2,
      120,
      2
    ],
    "array-callback-return": [
      1
    ],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "no-param-reassign": [
      1
    ],
    "no-underscore-dangle": [
      0
    ],
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "no-plusplus": [
      0
    ],
    "func-names": [
      0
    ],
    "no-console": [
      2
    ],
    "no-prototype-builtins": [
      1
    ],
    "no-useless-constructor": [
      0
    ],
    "no-else-return": [
      0
    ],
    "no-nested-ternary": [
      0
    ],
    "react/sort-comp": 0,
    "react/prop-types": [
      2,
      {
        "ignore": [
          "location",
          "children",
          "store",
          "history",
          "router",
          "dispatch"
        ]
      }
    ],
    "radix": [
      1
    ],
    "guard-for-in": [
      1
    ],
    "import/no-extraneous-dependencies": [
      1
    ],
    "arrow-body-style": [
      1
    ],
    "no-class-assign": [
      1
    ],
    "react/forbid-prop-types": [
      1
    ],
    "class-methods-use-this": [
      1
    ],
    "no-bitwise": [
      0
    ],
    "jsx-a11y/no-static-element-interactions": [
      1
    ],
    "react/prefer-stateless-function": [
      1
    ],
    "react/jsx-no-bind": [
      1
    ]
  }
}
